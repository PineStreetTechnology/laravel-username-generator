{
    "name": "pinestreettechnology/laravel-username-generator",
    "version": "1.0.0",
    "description": "Generate a random, kid-safe username. Forked from Grant Holle's original package.",
    "keywords": [
        "username",
        "username generator"
    ],
    "homepage": "https://github.com/PineStreetTechnology/laravel-username-generator",
    "license": "MIT",
    "authors": [
        {
            "name": "Digital Native Dev",
            "email": "no-reply@digitalnative.dev",
            "role": "Developer"
        },
        {
            "name": "Grant Holle",
            "email": "hollegrant@gmail.com",
            "role": "Original Author"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^10.0|^11.0",
        "illuminate/support": "^10.0|^11.0",
        "spatie/laravel-package-tools": "^1.9.2"
    },
    "require-dev": {
        "nunomaduro/collision": "^7.1|^8.1",
        "orchestra/testbench": "^8.0|^9.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "PineStreetTechnology\\UsernameGenerator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PineStreetTechnology\\UsernameGenerator\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": false,
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PineStreetTechnology\\UsernameGenerator\\UsernameServiceProvider"
            ],
            "aliases": {
                "Username": "PineStreetTechnology\\UsernameGenerator\\Facades\\Username"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
